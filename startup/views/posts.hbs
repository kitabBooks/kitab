<section>

<div class="container">
  
  <h1>{{user.name}}'s posts</h1>

  {{#if prevPage}}
    <div class = "col">
      <p>{{prevPage}}</p>
      <a href="/posts/{{queryStringPrev}}">Prev Page</a>
    </div>
  {{/if}}
    <div class="col">
    {{#each books}}
      {{> owneredBooks}}
    {{/each}}
    </div>
  {{#if nextPage}}
    <div class = "col">
      <p>{{nextPage}}</p>
      <a href="/posts/{{queryStringNext}}">Next Page</a>
    </div>
  {{/if}}
</div>

</section> 